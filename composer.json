        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MergimU\\Calculator\\CalculatorServiceProvider"
            ],
            "aliases": {
                "Calculator": "MergimU\\Calculator\\Facades\\Calculator"
            }
        }
    }
